#include<iostream>
using namespace std;

int main()
{
	int t;
	cin>>t;
	while(t--) {
		int n;
		 int count=8;
		cin>>n;
		while(n--)  {
			
			int a;
			cin>>a;
			if(a!=6 && a!=13 && a!=20 && a!=27 && a!=7 &&  a!=14 && a!=21 && a!=28)            // already holiday
			{
				count=count+1;
			}
		
		}
			cout<<count<<endl;
	}
	return 0;
}
